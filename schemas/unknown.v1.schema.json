{
  "$defs": {
    "DependsOn": {
      "additionalProperties": true,
      "properties": {
        "analysis_profile_digest": {
          "$ref": "#/$defs/Sha256"
        },
        "contracts": {
          "items": {
            "minLength": 1,
            "type": "string"
          },
          "type": "array"
        },
        "semantics_deviations": {
          "items": {
            "minLength": 1,
            "type": "string"
          },
          "type": "array"
        }
      },
      "type": "object"
    },
    "Expr": {
      "additionalProperties": true,
      "description": "Expression AST node. This schema intentionally allows additional fields to enable evolution; canonical form is defined by the DDD.",
      "properties": {
        "args": {
          "items": {
            "$ref": "#/$defs/ExprValue"
          },
          "type": "array"
        },
        "op": {
          "minLength": 1,
          "type": "string"
        },
        "pretty": {
          "type": "string"
        },
        "type": {
          "type": "string"
        }
      },
      "required": [
        "op"
      ],
      "type": "object"
    },
    "ExprValue": {
      "description": "Expression child node or value. Numbers MUST be integers in v1 (no float).",
      "oneOf": [
        {
          "$ref": "#/$defs/Expr"
        },
        {
          "type": "string"
        },
        {
          "type": "integer"
        },
        {
          "type": "boolean"
        },
        {
          "type": "null"
        }
      ]
    },
    "GeneratedAt": {
      "format": "date-time",
      "type": "string"
    },
    "IrAnchor": {
      "additionalProperties": false,
      "properties": {
        "block_id": {
          "pattern": "^B[0-9]+$",
          "type": "string"
        },
        "inst_id": {
          "pattern": "^I[0-9]+$",
          "type": "string"
        }
      },
      "required": [
        "block_id",
        "inst_id"
      ],
      "type": "object"
    },
    "Location": {
      "additionalProperties": false,
      "properties": {
        "col": {
          "minimum": 1,
          "type": "integer"
        },
        "file": {
          "$ref": "#/$defs/Path"
        },
        "line": {
          "minimum": 1,
          "type": "integer"
        },
        "offset": {
          "minimum": 0,
          "type": "integer"
        }
      },
      "required": [
        "file",
        "line",
        "col"
      ],
      "type": "object"
    },
    "MissingLemma": {
      "additionalProperties": false,
      "properties": {
        "expr": {
          "$ref": "#/$defs/Expr"
        },
        "notes": {
          "type": "string"
        },
        "pretty": {
          "type": "string"
        },
        "symbols": {
          "items": {
            "minLength": 1,
            "type": "string"
          },
          "type": "array"
        }
      },
      "required": [
        "expr",
        "pretty",
        "symbols"
      ],
      "type": "object"
    },
    "Path": {
      "description": "Path string. Canonicalization rules are specified outside JSON Schema.",
      "minLength": 1,
      "type": "string"
    },
    "RefinementAction": {
      "additionalProperties": false,
      "properties": {
        "action": {
          "minLength": 1,
          "type": "string"
        },
        "params": {
          "additionalProperties": true,
          "type": "object"
        }
      },
      "required": [
        "action"
      ],
      "type": "object"
    },
    "RefinementPlan": {
      "additionalProperties": false,
      "properties": {
        "actions": {
          "items": {
            "$ref": "#/$defs/RefinementAction"
          },
          "type": "array"
        },
        "message": {
          "type": "string"
        }
      },
      "required": [
        "message",
        "actions"
      ],
      "type": "object"
    },
    "RepoIdentity": {
      "additionalProperties": false,
      "properties": {
        "content_sha256": {
          "$ref": "#/$defs/Sha256"
        },
        "path": {
          "$ref": "#/$defs/Path"
        }
      },
      "required": [
        "path",
        "content_sha256"
      ],
      "type": "object"
    },
    "Sha256": {
      "description": "Content digest in the form sha256:<64 lowercase hex>",
      "pattern": "^sha256:[0-9a-f]{64}$",
      "type": "string"
    },
    "Tool": {
      "additionalProperties": false,
      "properties": {
        "build_id": {
          "type": "string"
        },
        "name": {
          "minLength": 1,
          "type": "string"
        },
        "version": {
          "minLength": 1,
          "type": "string"
        }
      },
      "required": [
        "name",
        "version"
      ],
      "type": "object"
    },
    "UnknownCode": {
      "description": "UNKNOWN classification code. v1 adopts the initial set from SRS Appendix A, and is extensible with versioning.",
      "minLength": 1,
      "type": "string"
    },
    "UnknownItem": {
      "additionalProperties": false,
      "properties": {
        "depends_on": {
          "$ref": "#/$defs/DependsOn"
        },
        "missing_lemma": {
          "$ref": "#/$defs/MissingLemma"
        },
        "po_id": {
          "$ref": "#/$defs/Sha256"
        },
        "refinement_plan": {
          "$ref": "#/$defs/RefinementPlan"
        },
        "unknown_code": {
          "$ref": "#/$defs/UnknownCode"
        },
        "unknown_stable_id": {
          "$ref": "#/$defs/Sha256"
        }
      },
      "required": [
        "unknown_stable_id",
        "po_id",
        "unknown_code",
        "missing_lemma",
        "refinement_plan"
      ],
      "type": "object"
    },
    "VersionString": {
      "minLength": 1,
      "pattern": "^[A-Za-z0-9_.-]+$",
      "type": "string"
    }
  },
  "$id": "sappp:schema/unknown.v1",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "generated_at": {
      "$ref": "#/$defs/GeneratedAt"
    },
    "input_digest": {
      "$ref": "#/$defs/Sha256"
    },
    "profile_version": {
      "$ref": "#/$defs/VersionString"
    },
    "proof_system_version": {
      "$ref": "#/$defs/VersionString"
    },
    "schema_version": {
      "const": "unknown.v1"
    },
    "semantics_version": {
      "$ref": "#/$defs/VersionString"
    },
    "tool": {
      "$ref": "#/$defs/Tool"
    },
    "tu_id": {
      "$ref": "#/$defs/Sha256"
    },
    "unknowns": {
      "items": {
        "$ref": "#/$defs/UnknownItem"
      },
      "type": "array"
    }
  },
  "required": [
    "schema_version",
    "tool",
    "generated_at",
    "tu_id",
    "unknowns",
    "semantics_version",
    "proof_system_version",
    "profile_version"
  ],
  "title": "SAP++ unknown.v1 (UNKNOWN ledger)",
  "type": "object"
}
